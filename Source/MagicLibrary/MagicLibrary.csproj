    </Compile>
    <Compile Update="Controls\TabControl\TabControl.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controls\TabControl\TabPage.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controls\WizardControl\WizardControl.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="Controls\WizardControl\WizardPage.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Docking\AutoHidePanel.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Docking\TabStub.cs">
