    <Compile Update="Docking\ZoneSequence.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Menus\MenuCommand.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Menus\MenuControl.cs">
      <SubType>Component</SubType>
    </Compile>
    <EmbeddedResource Include="Controls\InertButton.bmp" />
    <EmbeddedResource Include="Controls\TabbedGroups.bmp" />
    <EmbeddedResource Include="Controls\TabControl.bmp" />
    <EmbeddedResource Include="Controls\WizardControl.bmp" />
