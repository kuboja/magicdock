    <EmbeddedResource Include="Resources\ImagesPopupMenu.bmp" />
    <EmbeddedResource Include="Resources\ImagesTabbedGroups.bmp" />
    <EmbeddedResource Include="Resources\ImagesTabControl.bmp" />
    <EmbeddedResource Include="Resources\TabbedInvalid.cur" />
    <EmbeddedResource Include="Resources\TabbedValid.cur" />
    <EmbeddedResource Include="Resources\WizardPicture.bmp" />
    <Content Include="Readme.txt" />
    <Content Include="Resources\LibraryIcon.ico" />
  </ItemGroup>
  <PropertyGroup>
    <AssemblyTitle>Magic</AssemblyTitle>
    <Description>The User Interface Library for .NET</Description>
    <Company>Crownwood Consulting Ltd</Company>
    <Product>Magic</Product>
    <Copyright>Copyright 2002-2003 - Crownwood Consulting Ltd</Copyright>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>1.7.4.2</FileVersion>
